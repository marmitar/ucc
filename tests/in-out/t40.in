int main()
{
   int n, r = 0, t;

   print("Enter an integer to check if it's palindrome or not\n");
   read("%d", n);

   t = n;

   while (t != 0)
   {
      r = r * 10;
      r = r + t%10;
      t = t/10;
   }

   if (n == r)
      print("%d is a palindrome number.\n", n);
   else
      print("%d isn't a palindrome number.\n", n);

   return 0;
}
